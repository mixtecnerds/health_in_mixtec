{
  "metadata": {
    "title": "Dialog on difficulties with the knee",
    "source": "MedicalSpanish.com",
    "speaker": [
      "Rogelia"
    ]
  },
  "phrases": [
    {
      "translation": "Please point to the exact spot where it hurts.",
      "transcription": "Sa'a-un-ña ma̠ní sana̠'a-un ndáa nu̠ú u̠'vi̠",
      "media": "audio/2016_04_30_Rogelia_chest_pain_01.mp3"
    },
    {
      "translation": "Here.",
      "transcription": "Yó'o kú'vi̠-i.",
      "media": "audio/2016_04_30_Rogelia_chest_pain_02.mp3"
    },
    {
      "translation": "What does the pain feel like?",
      "transcription": "Na̠sa u̠'vi̠-ña, ká'án-un?",
      "media": "audio/2016_04_30_Rogelia_chest_pain_03.mp3"
    },
    {
      "translation": "It's like pressure on my chest.",
      "transcription": "Ndíka-yu ndúni̠'i.",
      "media": "audio/2016_04_30_Rogelia_chest_pain_04.mp3"
    },
    {
      "translation": "What were you doing when the pain started?",
      "transcription": "Ndáa ke sa'a-un tá ki̠xaá kú'vi̠-ña?",
      "media": "audio/2016_04_30_Rogelia_chest_pain_05.mp3"
    },
    {
      "translation": "Watching TV.",
      "transcription": "Téle va xi̠ndie'é-i",
      "media": "audio/2016_04_30_Rogelia_chest_pain_06.mp3"
    },
    {
      "translation": "How long have you had the pain?",
      "transcription": "Na̠sa óra kú'vi̠-un?",
      "media": "audio/2016_04_30_Rogelia_chest_pain_07.mp3"
    },
    {
      "translation": "For about three hours.",
      "transcription": "U̠ni̠ óra",
      "media": "audio/2016_04_30_Rogelia_chest_pain_08.mp3"
    },
    {
      "translation": "Is it difficult to breathe?",
      "transcription": "Á u̠'vi̠ ndaki'in-un ta̠chí-un?",
      "media": "audio/2016_04_30_Rogelia_chest_pain_09.mp3"
    },
    {
      "translation": "Yes.",
      "transcription": "Aan.",
      "media": "audio/2016_04_30_Rogelia_chest_pain_10.mp3"
    },

    {
      "translation": "I have high blood pressure.",
      "transcription": "Aan, ndú i'ní ni̠í-i.",
      "media": "audio/2016_04_30_Rogelia_chest_pain_12.mp3"
    },
    {
      "translation": "I’m going to do an electrocardiogram.",
      "transcription": "Ná tiaa so̠'o-i xí'in mákina ánima̠-un.",
      "media": "audio/2016_04_30_Rogelia_chest_pain_13.mp3"
    }
  ]
}
